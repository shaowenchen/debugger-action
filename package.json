{
  "name": "debugger-action",
  "version": "1.0.0",
  "private": true,
  "description": "A debugger for debugging actions online. It allows the user to login the action env and execute commands using ssh.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "all": "npm run build && npm run format && npm run lint && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shaowenchen/debugger-action.git"
  },
  "keywords": [
    "actions",
    "debugger",
    "online",
    "remote",
    "ssh"
  ],
  "author": "shaowenchen mail@chenshaowen.com https://www.chenshaowen.com/",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.0.4",
    "@actions/tool-cache": "^1.6.1",
    "ini": "^2.0.0",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@types/ini": "^1.3.30",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@typescript-eslint/parser": "^4.14.2",
    "@vercel/ncc": "^0.27.0",
    "eslint": "^7.19.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "js-yaml": "^4.0.0",
    "prettier": "2.2.1",
    "ts-jest": "^26.5.4",
    "typescript": "^4.1.3"
  }
}
